log4cplus.appender.WARN.layout.ConversionPattern=%D{%Y-%m-%d %H:%M:%S} [%t] %-5p [%l]: %m%n

log4cplus.appender.ERROR=log4cplus::DailyRollingFileAppender
log4cplus.appender.ERROR.Schedule=DAILY
